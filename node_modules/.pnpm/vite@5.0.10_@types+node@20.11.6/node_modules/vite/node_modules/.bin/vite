#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/asheleyine/github.com/coding-dojo/tailwindGenerator/node_modules/.pnpm/vite@5.0.10_@types+node@20.11.6/node_modules/vite/bin/node_modules:/home/asheleyine/github.com/coding-dojo/tailwindGenerator/node_modules/.pnpm/vite@5.0.10_@types+node@20.11.6/node_modules/vite/node_modules:/home/asheleyine/github.com/coding-dojo/tailwindGenerator/node_modules/.pnpm/vite@5.0.10_@types+node@20.11.6/node_modules:/home/asheleyine/github.com/coding-dojo/tailwindGenerator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/asheleyine/github.com/coding-dojo/tailwindGenerator/node_modules/.pnpm/vite@5.0.10_@types+node@20.11.6/node_modules/vite/bin/node_modules:/home/asheleyine/github.com/coding-dojo/tailwindGenerator/node_modules/.pnpm/vite@5.0.10_@types+node@20.11.6/node_modules/vite/node_modules:/home/asheleyine/github.com/coding-dojo/tailwindGenerator/node_modules/.pnpm/vite@5.0.10_@types+node@20.11.6/node_modules:/home/asheleyine/github.com/coding-dojo/tailwindGenerator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
