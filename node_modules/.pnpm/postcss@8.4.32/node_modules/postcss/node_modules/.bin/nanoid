#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/asheleyine/github.com/coding-dojo/tailwindGenerator/node_modules/.pnpm/nanoid@3.3.7/node_modules/nanoid/bin/node_modules:/home/asheleyine/github.com/coding-dojo/tailwindGenerator/node_modules/.pnpm/nanoid@3.3.7/node_modules/nanoid/node_modules:/home/asheleyine/github.com/coding-dojo/tailwindGenerator/node_modules/.pnpm/nanoid@3.3.7/node_modules:/home/asheleyine/github.com/coding-dojo/tailwindGenerator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/asheleyine/github.com/coding-dojo/tailwindGenerator/node_modules/.pnpm/nanoid@3.3.7/node_modules/nanoid/bin/node_modules:/home/asheleyine/github.com/coding-dojo/tailwindGenerator/node_modules/.pnpm/nanoid@3.3.7/node_modules/nanoid/node_modules:/home/asheleyine/github.com/coding-dojo/tailwindGenerator/node_modules/.pnpm/nanoid@3.3.7/node_modules:/home/asheleyine/github.com/coding-dojo/tailwindGenerator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nanoid@3.3.7/node_modules/nanoid/bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../../../../../nanoid@3.3.7/node_modules/nanoid/bin/nanoid.cjs" "$@"
fi
